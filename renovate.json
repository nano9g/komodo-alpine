{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^build\\.env$/"],
      "matchStrings": ["ALPINE_IMAGE_TAG=(?<currentValue>.*?)\\n"],
      "depNameTemplate": "rust",
      "datasourceTemplate": "docker"
    }
  ]
}
